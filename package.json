{
  "name": "proc-that-elastic-loader",
  "version": "0.1.0",
  "description": "Loader for proc-that. Loads processed items into an elasticsearch index.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf build",
    "pretest": "npm install && typings install && npm run clean && tsc",
    "test": "mocha --ui bdd --recursive ./build",
    "develop": "npm run clean && tsc -w",
    "build": "rimraf build dist && tsc --outDir dist -d && trash dist/**/*.spec*"
  },
  "keywords": [
    "etl",
    "node",
    "typescript",
    "proc-that",
    "elastic"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:buehler/proc-that-elastic-loader.git"
  },
  "bugs": "https://github.com/buehler/proc-that-elastic-loader/issues",
  "author": "Christoph BÃ¼hler <christoph.buehler@bluewin.ch>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "mocha": "^2.4.5",
    "proc-that": "^0.2.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "trash-cli": "^1.2.1",
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  },
  "dependencies": {
    "elasticsearch": "^10.1.3",
    "es6-promise": "^3.1.2",
    "rxjs": "^5.0.0-beta.4"
  }
}
